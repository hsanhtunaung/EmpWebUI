@model IEnumerable<EmployeeProj.Model.EmployeeResponseModel>

@{
    ViewBag.Title = "Employee List";
}

<h2>Employee List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EMPLOYEE_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMPLOYEE_NO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DOB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JOIN_DATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DEPARTMENT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SALARY)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SKILLS)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EMPLOYEE_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EMPLOYEE_NO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DOB)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JOIN_DATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DEPARTMENT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SALARY)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SKILLS)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EMPLOYEE_ID }) |        
            @Html.ActionLink("Delete", "Delete", new { id=item.EMPLOYEE_ID })
        </td>
    </tr>
}

</table>
